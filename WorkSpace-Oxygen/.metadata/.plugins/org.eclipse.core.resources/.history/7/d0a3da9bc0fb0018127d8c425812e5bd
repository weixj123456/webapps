package cn.temptation.domain;

public class Apple {
    String name;
        
     public Apple() {
		super();
		// TODO Auto-generated constructor stub
	}     
     
	public Apple(String name) {
		super();
		this.name = name;
	}



	public void setName(String name) {
		this.name = name;
	}

	String getName() {
    	
    	return name;
    }
	
	
	
}
